      CHA(ОБДНМСЛР) Q  B(1) PC(1) NA;     // Признак качества
      GRP ВИДОБЛ;                         // Вид облединения
        IND(5) ШИФР      PC(1);           // Шифр 
        MIT ОБДНВИДВ     B(1) PC(2);      // Шифр твердых отложений
        CHA(ОБДНВИДВ) Q  B(1) PC(1) NA;   // Признак качества
      END ВИДОБЛ;  
      MIT ОБДВРЕНВ     B(1) PC(2);        // Время начала случая облединения
      CHA(ОБДВРЕНВ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБДПРСТН     B(2) PC(4);        // Продолжительность стадии нарастания 
      CHA(ОБДПРСТН) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБДПРОДВ     B(2) PC(4);        // Продолжительность всего случая обледенения 
      CHA(ОБДПРОДВ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБДТЕМНМ     B(2) PC(5,1) D(1); // Температура воздуха в начале облединения
      CHA(ОБДТЕМНМ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБВЕТННМ     B(2) PC(3);        // Направление ветра в начал еобледенения
      CHA(ОБВЕТННМ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБВЕТСНМ     B(1) PC(3);        // Скорость ветра в начале обледенения 
      CHA(ОБВЕТСНМ) Q  B(1) PC(1) NA;     // Признак качества
      CHA(ОБВЕТСНМ) ОБДВПРНЮ B(1) PC(1);  // Признак наличия знака > у F
      MIT ОБДТЕМММ     B(2) PC(5,1)  D(1);// Темп-ра воздуха при достиж. отлож. макс. размера
      CHA(ОБДТЕМММ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБВЕТНММ     B(2) PC(3);        // Напр. ветра при достиж. отлож. макс. размера
      CHA(ОБВЕТНММ) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБВЕТСММ     B(1) PC(2);        // Скорость ветра при достиж. отлож. макс. размера
      CHA(ОБВЕТСММ) Q  B(1) PC(1);        // Признак качества
      CHA(ОБВЕТСММ) ОБДВМАНЮ B(1) PC(1);  // Признак наличия знака > у F
      MIT ОБДПРПЕР     B(1) PC(1);        // Признак перехода облед. через границу месяца
      CHA(ОБДПРПЕР) Q  B(1) PC(1) NA;     // Признак качества
      MIT ОБДПРД1Ч     B(2) PC(4);        // Продолжительность 1-й части облединения
      CHA(ОБДПРД1Ч) Q  B(1) PC(1) NA;     // Признак качества
      GRP СТАДИЯ;                         // Стадии нарастания отложений 
        IND(6) КСТАДИЙ   PC(1);           // Индекс
        MIT ОБДНДИАМ     B(2) PC(3);      // Диаметр отложения
        CHA(ОБДНДИАМ) Q  B(1) PC(1) NA;   // Признак качества 
        MIT ОБДТОЛЩМ     B(2) PC(3);      // Толщина отложения
        CHA(ОБДТОЛЩМ) Q  B(1) PC(1) NA;   // Признак качества  
        MIT ОБДНМАСТ     B(2) PC(5);      // Масса отложения
        CHA(ОБДНМАСТ) Q  B(1) PC(1) NA;   // Признак качества
      END СТАДИЯ;  
    END OBLED;                     
    GRV (СЧООЯ   ) OSOBO;          // Особо опасные явления
      IND(15) КООЯ     PC(2);             // Индекс 
      MIT ООЯЧИСЛН     B(1) PC(2);        // День начала ООЯ
      MIT ООЯВРЕМН     B(2) PC(5,2) D(2); // Время начала ООЯ (нат. знач. часы, мин.)
      CHA(ООЯВРЕМН) Q  B(1) PC(1);        // Признак качества
      MIT ООЯЧИСЛК     B(1) PC(2);        // День окончания ООЯ
      CHA(ООЯЧИСЛК) Q  B(1) PC(1);        // Признак качества
      MIT ООЯВРЕМК     B(2) PC(5,2) D(2); // Время окончания ООЯ (нат. знач. часы, мин.)
      CHA(ООЯВРЕМК) Q  B(1) PC(1);        // Признак качества
      MIT ООЯВИД       B(1) PC(2);        // Шифр ООЯ
      CHA(ООЯВИД  ) Q  B(1) PC(1);        // Признак качества
      MIT ООЯXАР1      B(2) PC(5);        // Первая характеристика ООЯ (точн до целого)
      CHA(ООЯXАР1 ) Q  B(1) PC(1);        // Признак качества
      MIT ООЯXАР2      B(2) PC(3);        // Вторая характеристика ООЯ
      CHA(ООЯXАР2 ) Q  B(1) PC(1);        // Признак качества
      MIT ООЯПРПЕР     B(1) PC(1);        // Признак перехода ООЯ через границу месяца  
      CHA(ООЯПРПЕР) Q  B(1) PC(1);        // Признак качества
      MIT ООЯПРО1Ч     B(3) PC(7,2) D(2); // Продолжительность 1-й части ООЯ (нат. знач. часы, мин.) 
      CHA(ООЯПРО1Ч) Q  B(1) PC(1);        // Признак качества
    END OSOBO;   
  END OBLJAW;  
 
  RBODY(9) ATMOS;                    // Данные об атмосферных явлениях
    KEY(I) ДЕНЬ      B(1) PC(2);            // День
    CNT СЧСРОК       B(1) PC(1);            // Число сроков, когда были АЯ
