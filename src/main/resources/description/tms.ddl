        CHA(АЯИНТЕНС) Q  B(1) PC(1) NA;     // Признак качества 
        MIT АЯВРЕМЯН     B(2) PC(5,2) D(2); // Время начала АЯ (нат.значение часы, мин) 
        CHA(АЯВРЕМЯН) Q  B(1) PC(1) NA;     // Признак качества
        MIT АЯВРЕМЯК     B(2) PC(5,2) D(2); // Время окончания АЯ (нат.значение часы, мин)
        CHA(АЯВРЕМЯК) Q  B(1) PC(1) NA;     // Признак качества
        CHA(АЯВРЕМЯК) АЯПРИЗПР B(1) PC(1);  // Признак прерыв. АЯ
      END АЯ;      
    END СРОК;    
  END ATMOS;   

  RBODY(10) CONPC;                  // Служебные данные о контроле
    GRP VEKTOR;
